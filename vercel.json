{
    "redirects": [ 
      {"source": "/URL", "destination": "/", "permanent": true }
    ],
    "rewrites": [
      { "source": "/", "destination": "/index.html"},
      { "source": "/:path((?!http|https).*)", "destination": "https://:path*" },
      { "source": "/https://:path*", "destination": "https://:path*" }
    ]
}