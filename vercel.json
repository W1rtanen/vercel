{
    "redirects": [ 
      {"source": "/URL", "destination": "https://www.proxies.dynv6.net", "permanent": true }
    ],
    "rewrites": [
      { "source": "/", "destination": "/index.html"},
      { "source": "/:match/:url*", "destination": "https://:match/:url*" },
      { "source": "/:match/:url*/", "destination": "https://:match/:url*/" },
      { "source": "/httpproxy/:match/:url*", "destination": "http://:match/:url*" },
      { "source": "/httpproxy/:match/:url*/", "destination": "http://:match/:url*/" }
    ]
}