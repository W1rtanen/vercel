{
    "redirects": [ 
      {"source": "/URL", "destination": "/", "permanent": true }
    ],
    "rewrites": [
      { "source": "/", "destination": "/index.html"},
      { "source": "/:path((http:\/\/).*)", "destination": "http://:path*" },
      { "source": "/:path((https:\/\/).*)", "destination": "https://:path*" },
      { "source": "/:path((?!http:\/\/|https:\/\/).*)", "destination": ":path*" }
    ]
}